<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
	<entry key="insertLiked">
		INSERT INTO LIKED`
		VALUES(?,?)
	</entry>
	

	<entry key="deleteLiked">
		DELETE FROM LIKED
		WHERE REF_UNO = ?
		AND REF_BNO = ?
	</entry>


	<entry key="insertDislike">
		INSERT INTO DISLIKE
		VALUES(?,?)
	</entry>
	

</properties>